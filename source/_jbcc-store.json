{
    "show": {
        "__exec": "echo \"=====[ {0} ]=====\";jq -r \".{0} | to_entries[] | \\\"\\(.key): \\(.value)\\\"\" ${__jbcc_store_path};echo \"==========================\""
    },
    "add": {
        "__exec": "jq \".{0}.\\\"{1}\\\" = \\\"{2}\\\"\" $__jbcc_store_path > tmp && mv tmp $__jbcc_store_path; _jbcc-store show {0}"
    },
    "complement": {
        "__exec": "echo `jq -r \".{0} | keys[] \" ${__jbcc_store_path} | xargs echo`"
    },
    "get": {
        "__exec": "jq -r \".{0}.\\\"{1}\\\"\" ${__jbcc_store_path}"
    },
    "remove": {
        "__exec": "jq \"del(.{0}.\\\"{1}\\\")\" $__jbcc_store_path > tmp && mv tmp $__jbcc_store_path; _jbcc-store show {0}"
    }
}