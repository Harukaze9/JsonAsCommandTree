{
    "root": {
        "git": {
            "update": {
                "__exec": "git submodule update --recursive "
            },
            "add-commit": {
                "__exec": "git add -u; git commit -m \\\"{0}\\\""
            },
            "fix-message": {
                "__exec": "git commit --amend -m \\\"{0}\\\""
            },
            "comb": {
                "__0": "echo -n $(git for-each-ref --format \"%(refname:lstrip=3)\" refs/remotes)",
                "__exec": "git checkout -fB my-{0} origin/{0}"
            }
        },
        "alias": {
            "__exec": "__jbcc_store -c alias -o list ",
            "list": {
                "__exec": "__jbcc_store -c alias -o list "
            },
            "add": {
                "__exec": "__jbcc_store -c alias -o add -k {0} -v \\\"{1}\\\""
            },
            "remove": {
                "__0": "__jbcc_store -c alias -o complement",
                "__exec": "__jbcc_store -c alias -o remove -k {0}"
            },
            "exec": {
                "__0": "__jbcc_store -c alias -o complement",
                "__exec": "__jbcc_store -c alias -o exec -k {0} -e \"JBCC_VALUE\""
            }
        },
        "clipboard": {
            "__exec": "__jbcc_store -c clipboard -o list ",
            "list": {
                "__exec": "__jbcc_store -c clipboard -o list "
            },
            "add": {
                "__exec": "__jbcc_store -c clipboard -o add -k {0} -v \\\"{1}\\\""
            },
            "remove": {
                "__0": "__jbcc_store -c clipboard -o complement",
                "__exec": "__jbcc_store -c clipboard -o remove -k {0}"
            },
            "exec": {
                "__0": "__jbcc_store -c clipboard -o complement",
                "__exec": "__jbcc_store -c clipboard -o exec -k {0} -e 'echo JBCC_VALUE' | clip.exe"
            }
        },
        "goto": {
            "__exec": "__jbcc_store -c goto -o list ",
            "list": {
                "__exec": "__jbcc_store -c goto -o list "
            },
            "add": {
                "__exec": "__jbcc_store -c goto -o add -k {0} -v $(pwd)"
            },
            "remove": {
                "__0": "__jbcc_store -c goto -o complement",
                "__exec": "__jbcc_store -c goto -o remove -k {0}"
            },
            "exec": {
                "__0": "__jbcc_store -c goto -o complement",
                "__exec": "__jbcc_store -c goto -o exec -k {0} -e 'cd JBCC_VALUE'"
            }
        },
        "note": {
            "__exec": "__jbcc_store -c note -o list ",
            "list": {
                "__exec": "__jbcc_store -c note -o list "
            },
            "add": {
                "__exec": "__jbcc_store -c note -o add -k {0} -v ~/jbcc_notes/{0}.md"
            },
            "remove": {
                "__0": "__jbcc_store -c note -o complement",
                "__exec": "__jbcc_store -c note -o remove -k {0}; rm -f ~/jbcc_notes/{0}"
            },
            "exec": {
                "__0": "__jbcc_store -c note -o complement",
                "__exec": "vim ~/jbcc_notes/{0}.md"
            }
        }
    }
}